void do_vfs(void)
{
  struct proc_dir_entry* proc_root = (void*) ((unsigned)kk_proc_root - CONFIG_PAGE_OFFSET);
  struct inode_operations *proc_iops = (void*) ((unsigned)proc_root->proc_iops - CONFIG_PAGE_OFFSET);
  printf("VFS check\n");

  //printf("orig lookup=%x lookup=%x %x\n", kk_proc_lookup, proc_root->proc_iops->lookup, proc_root);
  if((unsigned)(proc_iops->lookup) != (unsigned)kk_proc_root_lookup)
    printf("Possible RK infection! %x != %x[orig] %s\n", proc_iops->lookup, kk_proc_root_lookup, (unsigned)(proc_iops->lookup) > LAST_ADDR ? "[!T]" : "[T]"); 

  printf("orig lookup_fnc=%x lookup=%x root=%x iops=%x\n", kk_proc_root_lookup, proc_iops->lookup, proc_root, proc_iops);

  while(1);
}
